0022 rrr =  substr(yyy,5)                   /*returns  efghi                 */
0023 mmm = 'SUBSTR'(yyy,5)                  /*returns  56789abcdefgji        */
0024 sss = "SUBSTR"(yyy,5)                  /* (same as above)               */
0025 exit                                   /*stick a fork in it, we're done.*/
