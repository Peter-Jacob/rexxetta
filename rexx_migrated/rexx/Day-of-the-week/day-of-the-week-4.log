0011 dow: procedure; parse arg m,d,y;                 if m<3  then do;  m= m+12;  y= y-1;  end
0012 yL= left(y, 2);      yr= right(y, 2);  w= (d + (m+1)*26%10 +yr +yr%4 +yL%4 +5*yL) //7
0013 if w==0  then w= 7;  return w               /*Sunday=1,  Monday=2,  ···  Saturday=7*/
